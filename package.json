{
  "name": "font-changer",
  "displayName": "font-changer",
  "description": "Change fonts via twitch channel points",
  "version": "0.0.1",
  "publisher": "VSCodeFontChanger",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:font-changer.signInTwitch",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "font-changer.signInTwitch",
        "title": "Font Changer: Sign In With Twitch"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.54.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "font-list": "^1.2.12",
    "fonts-installed-osx": "^1.0.0",
    "keytar": "^7.4.0",
    "twitch": "^4.5.1",
    "twitch-auth": "^4.5.1",
    "twitch-pubsub-client": "^4.5.1",
    "utf-8-validate": "^5.0.4"
  }
}
